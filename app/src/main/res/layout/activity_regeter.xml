<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_regeter"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.administrator.day03.login.regeter">

    <TextView
        android:gravity="center"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:textSize="30dp"
      android:text="用户名:"
      android:id="@+id/textView2" />

    <AutoCompleteTextView
        android:text="1"
        android:layout_below="@+id/autoCompleteTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/autoCompleteTextView2"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView2" />

    <AutoCompleteTextView
        android:text="1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/autoCompleteTextView"
        android:layout_alignTop="@+id/textView3"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/autoCompleteTextView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:gravity="center"

        android:text="密码:"
        android:layout_marginTop="20dp"
        android:id="@+id/textView3"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true" />

    <Button
        android:text="注册"
        android:textSize="25dp"
        android:textColor="#ffffff"
        android:background="#3F51B5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/autoCompleteTextView"
        android:layout_alignParentStart="true"
        android:id="@+id/regeter11"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="100dp"

        android:layout_height="100dp"
        android:id="@+id/Touimage"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/regeter11"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
